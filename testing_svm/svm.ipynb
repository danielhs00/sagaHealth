# Definisi fungsi untuk interpretasi hasil
def interpretasi_risiko(probabilitas):
    if probabilitas < 0.3:
        return "RENDAH", "hijau"
    elif probabilitas < 0.6:
        return "SEDANG", "kuning"
    else:
        return "TINGGI", "merah"

